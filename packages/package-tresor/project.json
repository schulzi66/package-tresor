{
  "root": "packages/package-tresor",
  "sourceRoot": "packages/package-tresor/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/package-tresor",
        "main": "packages/package-tresor/src/index.ts",
        "tsConfig": "packages/package-tresor/tsconfig.app.json",
        "assets": ["packages/package-tresor/*.md"]
      }
    },
    "serve": {
      "executor": "@nrwl/js:node",
      "options": {
        "buildTarget": "package-tresor:build"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/package-tresor/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/packages/package-tresor"],
      "options": {
        "jestConfig": "packages/package-tresor/jest.config.js",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
